[application]

config/name="GravityGame"
run/main_scene="res://scenes/main_menu.tscn"
config/icon="res://icon.png"

[autoload]

Physics="*res://scripts/auto_load/physics.gd"
StaticEffect="*res://scripts/auto_load/static_effect.gd"
Fullscreen="*res://scripts/auto_load/fullscreen.gd"
Pause="*res://scripts/auto_load/pause.gd"

[display]

window/size/fullscreen=true

[editor_plugins]

enabled=PoolStringArray( "res://addons/qodot/plugin.cfg" )

